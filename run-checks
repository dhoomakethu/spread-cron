#!/bin/sh

git clone https://github.com/snapcore/spread-images
cp sa.json spread-images/sa.json

. ./options
last_build_number=$(echo "$TRAVIS_COMMIT_MESSAGE" | perl -n -e '/'"$message"' \((.*?)\)/ && print $1' | cut -d+ -f1 | cut -d~ -f1)

git checkout "${last_build_number}"

./spread-images/run_google_task.sh update-ubuntu-16.04-64
