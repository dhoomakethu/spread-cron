pattern_extractor="curl -v -s -H 'X-Ubuntu-Architecture: amd64' -H 'X-Ubuntu-Series: 16' https://search.apps.ubuntu.com/api/v1/snaps/details/core?channel=candidate 1>$SNAP_COMMON/snapd-core-candidate.last.response 2>>$SNAP_COMMON/snapd-core-candidate.log && (cat $SNAP_COMMON/snapd-core-candidate.last.response; echo) >> $SNAP_COMMON/snapd-core-candidate.log && cat $SNAP_COMMON/snapd-core-candidate.last.response | jq -j '.revision'"
message="New amd64 core snap in candidate channel"
